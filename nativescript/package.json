    "nativescript-dev-android-snapshot": "^0.0.10",
    "nativescript-dev-sass": "^1.3.2",
    "nativescript-dev-typescript": "~0.5.1",
    "nativescript-dev-webpack": "^0.7.3",
    "raw-loader": "~0.5.1",
    "resolve-url-loader": "~2.0.2",
